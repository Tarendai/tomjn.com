{
  "name": "tomjn.com",
  "version": "1.0.0",
  "description": "The WP theme for tomjn.com",
  "scripts": {
    "build:scss": "node-sass assets/scss/frontend.scss assets/dist/frontend.css && node-sass assets/scss/editor.scss assets/dist/editor.css",
    "build:less": "lessc style.less style.css && lessc editor-style.less editor-style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomjn/tomjn.com.git"
  },
  "keywords": [
    "wordpress",
    "theme"
  ],
  "author": "Tom J Nowell",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tomjn/tomjn.com/issues"
  },
  "homepage": "https://github.com/tomjn/tomjn.com#readme",
  "require": {
    "scss": "*"
  },
  "devDependencies": {
    "bulma": "^0.8.1",
    "less": "^3.11.1",
    "scss": "^0.2.4"
  }
}
